    "scaler = StandardScaler()\n",
    "Xs = scaler.fit_transform(X)\n",
    "\n",
    "# Dimensionality Reduction using PCA to reduce features to 2 components\n",
    "pca = PCA(n_components=2)\n",
    "\n",
    "# USING PCA for all\n",
    "#pca.fit(Xs)\n",
